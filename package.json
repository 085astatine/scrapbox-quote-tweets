  "scripts": {
    "chrome:build": "TARGET_BROWSER=chrome webpack --mode development",
    "chrome:build:production": "TARGET_BROWSER=chrome webpack --mode production",
    "chrome:build:artifact": "web-ext build --source-dir=build/production/chrome --filename={name}-{version}-chrome.zip",
    "chrome:run": "PATH=./bin:$PATH TARGET_BROWSER=chrome web-ext run --target=chromium --source-dir=build/development/chrome",
    "chrome:run:production": "PATH=./bin:$PATH TARGET_BROWSER=chrome web-ext run --target=chromium --source-dir=build/production/chrome",
    "chrome:watch": "TARGET_BROWSER=chrome webpack --watch --mode development",
    "firefox:build": "TARGET_BROWSER=firefox webpack --mode development",
    "firefox:build:production": "TARGET_BROWSER=firefox webpack --mode production",
    "firefox:build:artifact": "web-ext build --source-dir=build/production/firefox --filename={name}-{version}-firefox.zip",
    "firefox:run": "TARGET_BROWSER=firefox web-ext run --target=firefox-desktop --source-dir=build/development/firefox",
    "firefox:run:production": "TARGET_BROWSER=firefox web-ext run --target=firefox-desktop --source-dir=build/production/firefox",
    "firefox:watch": "TARGET_BROWSER=firefox webpack --watch --mode development",
    "build": "run-p *:build",
    "build:production": "run-p *:build:production",
    "build:artifact": "run-p *:build:artifact",
    "test": "jest",
    "clean": "rimraf build/ web-ext-artifacts/",
    "eslint": "eslint src/ test/",
    "prettier:check": "prettier --check src/ test/",
    "prettier:write": "prettier --write src/ test/",
    "stylelint:check": "stylelint src/**/*.scss",
    "stylelint:write": "stylelint --fix src/**/*.scss",
